{
  "name": "anti_theft",
  "version": "1.0.0",
  "private": true,
  "description": "Anti Theft",
  "scripts": {
    "serve": "vite preview",
    "build-new": "vite build",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "dev2": "vite",
    "dev": "npx electronmon .",
    "preview": "vite preview"
  },
  "electronmon": {
    "patterns": ["!src/assets/**"]
  },
  "main": "dist-electron/main/index.js",
  "dependencies": {
    "@mdi/font": "5.9.55",
    "koffi": "^2.8.0",
    "loudness": "^0.4.2",
    "roboto-fontface": "*",
    "vuetify": "^3.0.0-beta.0",
    "vuex": "^4.0.2",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@types/webfontloader": "^1.0.0",
    "@vitejs/plugin-vue": "^5.0.1",
    "autoprefixer": "^10.4.18",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.35",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.1",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^1.8.27"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "license": "MIT",
  "type": "module"
}
